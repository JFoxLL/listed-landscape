<table class="performance-table-individual">
    <thead>
        <tr>
            <th>Updated: <%= lic.performance_last_updated.strftime("%B %Y") %></th>
            <th>1yr </th>
            <th>3yrs</th>
            <th>5yrs</th>
            <th>10yrs</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>With Dividends Reinvested - Net</td>
            <td><%= lic.performance_cagr_calculation_rounded_1dp(1, :cash_amount) %></td>
            <td><%= lic.performance_cagr_calculation_rounded_1dp(3, :cash_amount) %></td>
            <td><%= lic.performance_cagr_calculation_rounded_1dp(5, :cash_amount) %></td>
            <td><%= lic.performance_cagr_calculation_rounded_1dp(9.9, :cash_amount) %></td>
        </tr>
        <tr>
            <td>With Dividends Reinvested - Gross</td>
            <td><%= lic.performance_cagr_calculation_rounded_1dp(1, :grossed_up_amount) %></td>
            <td><%= lic.performance_cagr_calculation_rounded_1dp(3, :grossed_up_amount) %></td>
            <td><%= lic.performance_cagr_calculation_rounded_1dp(5, :grossed_up_amount) %></td>
            <td><%= lic.performance_cagr_calculation_rounded_1dp(9.9, :grossed_up_amount) %></td>
        </tr>
    </tbody>
</table>