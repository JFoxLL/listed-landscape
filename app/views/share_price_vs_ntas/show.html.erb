<!-- SEO details -->
<% content_for :title do %>
  NTA | <%= @lic.name %> (ASX:<%= @lic.ticker %>)
<% end %>

<% content_for :description do %>
  [<%= Time.now.strftime("%b %Y") %> Updated] Net Tangible Assets (NTA) for <%= @lic.name %> (ASX: <%= @lic.ticker %>). Including Comparison to Share Price, to Determine Premium/ Discount.
<% end %>

<% content_for :keywords do %>
  <%= @lic.name %>, <%= @lic.ticker %>, NTA, Net Tangible Assets
<% end %>

<!-- Back to Lic Show Page Button -->
<%= render partial: "shared/l3_seo_page_back_button" %>

<!-- Page Header -->
<div class="seo-pages-header">
  <h1><%= @lic.ticker %> - Net Tangible Assets (NTA)</h1>
</div>
<div class="seo-pages-page-details">
  <p>Net Tangible Asset (NTA) information for <%= @lic.name %> (ASX: <%= @lic.ticker %>)</p>
</div>

<!-- Separator -->
<%= render partial: 'shared/l3_seo_page_separator' %>

<!-- Back to Lic Show Page Section -->
<%= render partial: 'shared/l3_seo_page_back_section' %>